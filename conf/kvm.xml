<domain type = 'kvm'>
    <name>T_NAME</name>
    <memory>T_MEMORY</memory>
    <vcpu>T_VCPU</vcpu>
    <os>
        <type arch = 'x86_64' machine = 'pc'>hvm</type>
        <boot dev = 'T_BOOT'/>
    </os>
    <features>
        <acpi/>
        <apic/>
        <pae/>
    </features>
    <clock offset = 'localtime'/>
    <on_poweroff>destroy</on_poweroff>
